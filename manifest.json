{
    "name": "Skribbl Bamboozle",
    "version": "1.0",
    "description": "Skribbl.io script",
    "permissions": [
        "activeTab",
        "declarativeContent"
    ],
    "background": {
        "scripts": [
            "src/rules.js"
        ],
        "persistent": false
    },
    "page_action": {
        "default_popup": "src/popup.html",
        "default_icon": {
            "42": "img/favicon.png"
        }
    },
    "web_accessible_resources": [
        "src/words.js"
    ],
    "manifest_version": 2
}